<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MenuViewTool">
        <attr name="titleName" format="string" />
    </declare-styleable>

    <attr name="relation">
        <enum name="icon_left" value="0" />
        <enum name="icon_right" value="1" />
        <enum name="icon_above" value="2" />
        <enum name="icon_below" value="3" />
        <enum name="icon_null" value="4" />
    </attr>
    <attr name="position">
        <enum name="frame_left" value="5" />
        <enum name="frame_right" value="6" />
        <enum name="frame_top" value="7" />
        <enum name="frame_bottom" value="8" />
        <enum name="frame_null" value="9" />
        <enum name="frame_all" value="10" />
    </attr>

    <declare-styleable name="iconbutton">
        <attr name="relation" />
        <attr name="icon" format="reference" />
        <attr name="mtext" format="string" />
        <attr name="text_size" format="dimension" />
        <attr name="hasicon" format="boolean" />
        <attr name="split_number" format="integer" />
    </declare-styleable>
    <declare-styleable name="iconframe">
        <attr name="position" />
        <attr name="frameposition" format="reference" />
    </declare-styleable>

</resources>